{
    "id": "gcloud-cli",
    "version": "0.0.1",
    "name": "gcloud-cli",
    "documentationURL": "https://github.com/hayas1/devcontainer-features/tree/main/src/gcloud-cli",
    "description": "Install gcloud-cli with kubectl and helm (optional)",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "433.0.1-0",
                "323.0.0-0"
            ],
            "default": "latest",
            "description": "install version of gcloud-cli"
        },
        "with-kubectl": {
            "type": "string",
            "proposals": [
                "none",
                "latest",
                "1.27.2-00"
            ],
            "default": "none",
            "description": "install version of kubectl or none"
        },
        "with-helm": {
            "type": "string",
            "proposals": [
                "none",
                "latest",
                "3.12.0-1"
            ],
            "default": "none",
            "description": "install version of helm or none"
        },
        "completion": {
            "type": "string",
            "proposals": [
                "zsh",
                "bash"
            ],
            "default": "zsh",
            "description": "completion setting for selected shell"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils",
        "ghcr.io/hayas1/devcontainer-features/starship-sheldon"
    ]
}