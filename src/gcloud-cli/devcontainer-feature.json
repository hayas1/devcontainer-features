{
    "id": "gcloud-cli",
    "version": "0.0.1",
    "name": "gcloud-cli",
    "documentationURL": "https://github.com/hayas1/devcontainer-features/tree/main/src/gcloud-cli",
    "description": "Install gcloud-cli",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "master",
            "description": "install version of gcloud-cli"
        },
        "with-kubectl": {
            "type": "string",
            "proposals": [
                "none",
                "latest"
            ],
            "default": "none",
            "description": "install version of kubectl or none"
        },
        "with-helm": {
            "type": "string",
            "proposals": [
                "none",
                "latest"
            ],
            "default": "none",
            "description": "install version of helm or none"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}