{
    "install_minimal": {
        "image": "mcr.microsoft.com/vscode/devcontainers/base:0-bullseye",
        "features": {
            "zsh-dotfiles": {
                "write-zshrc": false
            }
        }
    },
    "install_all": {
        "image": "mcr.microsoft.com/vscode/devcontainers/base:0-bullseye",
        "features": {
            "zsh-dotfiles": {
                "with-starship-config": true,
                "with-sheldon-config": true,
                "with-some-tools": true,
                "overwrite-zshrc": true
            }
        }
    }
}